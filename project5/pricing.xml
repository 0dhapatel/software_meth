<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Pricing">

    <TextView
        android:id="@+id/ticketprice3"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:text="Sales Tax"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="668dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/ticketprice2"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:text="Ticket Total"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="730dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="64dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Senior"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="406dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="77dp"
        android:layout_height="41dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Student"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="471dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="TextView"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calculate Ticket Price"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="519dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="280dp"
        android:layout_height="255dp"
        app:srcCompat="@drawable/montclair_art_museum__montclair__new_jersey_"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="72dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="64dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Adult"
        tools:layout_editor_absoluteX="74dp"
        tools:layout_editor_absoluteY="353dp"
        tools:ignore="MissingConstraints" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="103dp"
        android:layout_height="23dp"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="340dp"
        tools:ignore="MissingConstraints" />

    <resources tools:ignore="MissingConstraints">
        <string-array android:name="planets_array">
            <item>1</item>
            <item>2</item>
            <item>3</item>
            <item>4</item>
            <item>5</item>
        </string-array>
    </resources>

    <TextView
        android:id="@+id/ticketprice"
        android:layout_width="172dp"
        android:layout_height="46dp"
        android:text="Ticket Price"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="606dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
